	<div id=inqList>

 		    <table>
		    	<thead>
			  <tr>
			    <th></th>
			    <th>部材コード</th>
			    <th>搬入数量</th>
	    		    <th>ロットNo</th>
	    		    <th>部材名</th>
	    		    <th>搬入コード</th>
	    		    <th>特記事項</th>
	    		    <th>業者メモ</th>
	    		    </tr>
			    </thead>
			    <tbody>
			    <% for (var i = 0; i < tab.length; i++) { %>
			    <TR>
			    <TD>
			    <% if (tab[i].sqty >1000) { %>
			       <input type='radio' name='rdo01' checked > 
			    <% } else { %>
			       <input type='radio' name='rdo01'> 
			    <% } %>	  
			    </TD>
			    <TD><%= tab[i].pcode %></TD>
			    <TD><%= tab[i].sqty %></TD>
			    <TD><%= tab[i].lotn %></TD>
			    <TD><%= tab[i].mem1 %></TD>
			    <TD><%= tab[i].mem2 %></TD>
			    <TD><%= tab[i].mem3 %></TD>
			    <TD><%= tab[i].pnam %></TD>
			    </TR>   
			    <% } %>
			    </tbody>
			    </table>

	
			</TABLE>
