<div id=inqList>
  <table>
	<thead>
	  <tr>
		<th></th>
		<th>会员号码</th>
		<th>数量</th>
	    <th>橱柜号码</th>
	    <th>标签号码</th>
	    <th>储蓄期间</th>
	    <th>特别记载事项</th>
	    <th>顾客笔记</th>
	  </tr>
	</thead>
	<tbody>
	  <% for (var i = 0; i < tab.length; i++) { %>
		 <TR>
		   <TD>
			 <% if (tab[i].sqty >1000) { %>
	  <input type='radio' name='rdo01' value='<%= tab[i].pcode %>' onclick='pcode.value=this.value;' checked > 
	  <% } else { %>
	  <input type='radio' name='rdo01' value='<%= tab[i].pcode %>' onclick='pcode.value=this.value;'> 
	  <% } %>	  
</TD>
<TD><%= tab[i].pcode %></TD>
<TD><%= tab[i].sqty %></TD>
<TD><%= tab[i].pnam %></TD>
<TD><%= tab[i].lotn %></TD>
<TD><%= tab[i].mem1 %></TD>
<TD><%= tab[i].mem2 %></TD>
<TD><%= tab[i].mem3 %></TD>
</TR>   
<% } %>
</tbody>
</table>

	
</TABLE>
</div> <!-- inqList -->


</div> <!-- main -->
