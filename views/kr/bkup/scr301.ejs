<div id="contents">

    <div id="mainx">
    <form name="main_form" method="post" action="/scr/301" class="regist3">
        <section id="mailTray">
            <h1>CheckOut</h1>
            <div id="operationPanel">
            <span class="<%= mesg_level_color %>"><%= mesg %></span>
            </div><!-- operationPanel -->
        </section> <!-- mailTray -->

        <div id="ordrList">
            <table border="0">
              <col width="110" align="center">
              <col width="285" align="left">
              <col width="110" align="center">
              <col width="" align="left">
              <tr>
                <th>오더 번호</th>
                <td>
                  <INPUT type="text" id="ordr" class="STYLE_ORDR" size="20" maxlength="20" 
                   placeholder="prefix search" name="ordr" 
                   value="<%= text.ordr %>" tabindex = "-1">
                </td>
                <th>상태</th>
                <td>
                  <INPUT type="checkbox" id="c1" class="" name="chk1" value="1" 
                   <%= checkbox.chk1['1'] %>>시작전
                  <INPUT type="checkbox" id="c1" class="" name="chk1" value="2" 
                   <%= checkbox.chk1['2'] %>>시작
                  <INPUT type="checkbox" id="c1" class="" name="chk1" value="3" 
                   <%= checkbox.chk1['3'] %>>종료
                </td>
              </tr>
              <tr>
                <th>회원번호</th>
                <td>
                  <INPUT type="text" id="lotn" class="STYLE_LOTN" size="8" maxlength="8" 
                   placeholder="prefix search" name="lotn" 
                   value="<%= text.lotn %>" tabindex = "-1">
                </td>
                <th></th>
                <td></td>
              </tr>
              <tr>
                <th>보관 장소</th>
                <td>
                  <select name="area">
                    <option value=""></option><!--dmy-->
                    <% for (var i=0,max=options.length; i < max; i++ ) { %>
                      <option value="<%= options[i].area %>" <%= options[i].selected %>>
                        <%= options[i].area_name %>
                      </option>
                    <% } %>
                  </select>
                </td>
                <th></th>
                <td></td>
              </tr>
            </table>
        </div> <!-- ordrList -->

        <br/>

        <div id="inqList2">
            <table>
                <col width="50">
                <col width="180">
                <col width="100">
                <col width="100">
                <col width="80">
                <col width="">
                <tr>
                    <th></th>
                    <th>오더 번호</th>
                    <th>회원번호</th>
                    <th>수량</th>
                    <th>상태</th>
                    <th>비고</th>
                </tr>

                <% for (var i = 0, max=tables.length  ; i < max ; i++ ) { %>
                <tr>
                    <td>
                    <% if (tables[i].radio == "1") { %>
                      <input type='radio' name='rad01' value="<%= tables[i].ordr %>"
                         <%= tables[i].radio_checked %>>
                    <% } %>
                    </td>
                    <td><%= tables[i].ordr %></td>
                    <td><%= tables[i].lotn %></td>
                    <td><%= tables[i].sqty %></td>
                    <td><%= tables[i].stat_str %></td>
                    <td><%= tables[i].mem1 %></td>
                </tr>
                <% } %>

            </table>
        </div><!--inqList-->

        <br/>

		<h2>오더 번호:<%= tables2_head.ordr %></h2>
		<h2>시작 시간 :<%= tables2_head.start %></h2>

        <div id="inqList2">
            <table>
                <col width="180">
                <col width="100">
                <col width="100">
                <col width="100">
                <col width="">
                <tr>
                    <th>로커 번호</th>
                    <th>태그 번호</th>
                    <th>출고 시간</th>
                    <th>기간</th>
                    <th>비고</th>
                </tr>

                <% for (var i = 0, max=tables2.length  ; i < max ; i++ ) { %>
                <tr>
                    <td><%= tables2[i].locn_name %></td>
                    <td><%= tables2[i].part %></td>
                    <td><%= tables2[i].part_name %></td>
                    <td><%= tables2[i].sqty %></td>
                    <td><%= tables2[i].mem1 %></td>
                </tr>
                <% } %>

            </table>
        </div><!--inqList-->

        <div id="step_info"></div>


    </div> <!--mainx-->

    <div id="sub">
        <section id="createMail">
            <p><a href="#">체크아웃(check-out)</a></p>
        </section>
        <section id="folderList" class='regist'>
