/* inqList2 */
#inqList2 {
    border-bottom: 2px solid #D6E1C7;
}
#inqList2 table {
    width: 1024px;
    table-layout:fixed;
}
#inqList2 th,
#inqList2 td {
    padding: 3px 5px;
}
#inqList2 th {
    background: #f8f8f8;
    font-weight: normal;
    text-align: left;
}
#inqList2 th:first-child {
    width: 1%;
}
#inqList2 td {
    border-top: 1px solid #eee;
    cursor: pointer;
}
#inqList2 .current td {
    background: #f4f9ed;
    font-weight: bold;
}

/* add 2012.06.30 for SCR103 */
table.iqy103_mon {
    table-layout:fixed;
    width:800px;
    border:2px;
    border-style:solid;
}
table.iqy103_mon tr {
    border:2px;
    border-style:solid;
}
table.iqy103_mon th {
    color:#403B33;
    background-color:#55BBAA;
    padding:5px 0;
    border:2px;
    border-style:solid;
}
table.iqy103_mon th span {
    font-weigth:bold;
    font-size:18px;
    font-style:'Questrial', sans-serif;
}
table.iqy103_mon td {
    color:#403B33;
    background-color:#f0f0f0;
    text-align:center;
    padding:5px 0;
    border:2px;
    border-style:solid;
}
table.iqy103_mon td span{
    font-weigth:normal;
    font-size:72px;
    font-style:'Questrial', sans-serif;
}
