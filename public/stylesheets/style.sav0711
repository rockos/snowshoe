}
#mailDetail header li span {
	margin: 0 5px 0 0;
	color: #999;
}
#mailDetail header p {
	position: absolute;
	top: 0;
	right: 0;
}
#mailDetail .body {
	line-height: 1.3;
}

/* --------------------------------------------------
	以下、サンプルコード
-------------------------------------------------- */

#contents {
	/* Old */
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: box;
	/* New */
	display: -webkit-flexbox;
	display: -moz-flexbox;
	display: -ms-flexbox;
	display: flexbox;
	
	width: 100%;
}
#mainx {
	/* Old */
	-webkit-box-ordinal-group: 2;
	-moz-box-ordinal-group: 2;
	-ms-box-ordinal-group: 2;
	box-ordinal-group: 2;
	/* New */
	-webkit-flex-order: 2;
	-moz-flex-order: 2;
	-ms-flex-order: 2;
	flex-order: 2;
	
	/* Old */
	-webkit-box-flex: 1;
	-moz-box-flex: 1;
	-ms-box-flex: 1;
	box-flex: 1;
	/* New */
	width: -webkit-flex(1);
	width: -moz-flex(1);
	width: -ms-flex(1);
	width: flex(1);
	
	background: #fff;
	word-wrap: break-word;
}
#sub {
	/* Old */
	-webkit-box-ordinal-group: 1;
	-moz-box-ordinal-group: 1;
	-ms-box-ordinal-group: 1;
	box-ordinal-group: 1;
	/* New */
	-webkit-flex-order: 1;
	-moz-flex-order: 1;
	-ms-flex-order: 1;
	flex-order: 1;
	
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	
	width: 150px;
	padding: 10px;
	background: #edebe6;
}
#subMenu {
	float: left;
}

/* operationPanel */
#subMenu {
	overflow: hidden;
	padding: 5px 5px 0;
#	background: #D6E1C7;
#	background: #45C;
}
#subMenu ul {
	float: left;
	margin: 0 10px 0 0;
}
#subMenu li {
	float: left;
	margin: 0 5px 0 0;
	padding: 0 0 5px 0;
}
#subMenu a {
	display: block;
	padding: 5px;
	background: #fff;
	border-radius: 3px;
	color: #403B33;
	text-decoration: none;
}
#extra {
	/* Old */
	-webkit-box-ordinal-group: 3;
	-moz-box-ordinal-group: 3;
	-ms-box-ordinal-group: 3;
	box-ordinal-group: 3;
	/* New */
	-webkit-flex-order: 3;
	-moz-flex-order: 3;
	-ms-flex-order: 3;
	flex-order: 3;
	
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	
	width: 200px;
	padding: 10px;
	background: #eee;
}
@media screen and (max-width: 800px) {
	#extra {
		display: none;
	}
}

