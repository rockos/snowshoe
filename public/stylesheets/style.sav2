/* //////////////////////////////////////////////////
[Module] 
////////////////////////////////////////////////// */

/* createMail */
#createMail {
	margin: 0 0 10px;
}
#createMail h1 {
	display: none;
}
#createMail a {
	display: block;
	padding: 10px;
	background: #D3643B;
	border-radius: 3px;
	color: #fff;
	font-weight: bold;
	text-align: center;
	text-decoration: none;
}

/* folderList */
#folderList h1 {
	display: none;
}
#folderList a {
	display: block;
	position: relative;
	margin: 0 0 5px;
	padding: 7px;
	background: #fff;
	border-radius: 3px;
	color: #403B33;
	text-decoration: none;
}
#folderList .current a {
	font-weight: bold;
}
#folderList .current a:after {
	content: "►";
	position: absolute;
	top: 50%;
	right: 7px;
	margin: -.4em 0 0;
	color: #D3643B;
	font-size: 10px;
}

/* ad */
#ad {
	line-height: 1.2;
}
#ad dt {
	margin: 0 0 5px;
	font-size: 14px;
	font-weight: bold;
}
#ad dt a {
	color: #15C;
	text-decoration: none;
}
#ad .txt {
	margin: 0 0 5px;
}
#ad .url {
	margin-bottom: 20px;
	font-size: 12px;
	font-weight: bold;
}
#ad .url a {
	color: #093;
	text-decoration: none;
}

/* mailTray */
#mailTray h1 {
	display: none;
}

/* operationPanel */
#operationPanel {
	overflow: hidden;
	padding: 5px 5px 0;
	background: #D6E1C7;
}
#operationPanel ul {
	float: left;
	margin: 0 10px 0 0;
}
#operationPanel li {
	float: left;
	margin: 0 5px 0 0;
	padding: 0 0 5px 0;
}
#operationPanel a {
	display: block;
	padding: 5px;
	background: #fff;
	border-radius: 3px;
	color: #403B33;
	text-decoration: none;
}

/* mailList */
#mailList {
	border-bottom: 2px solid #D6E1C7;
}
#mailList table {
	width: 100%;
}
#mailList th,
#mailList td {
	padding: 3px 5px;
}
#mailList th {
	background: #f8f8f8;
	font-weight: normal;
	text-align: left;
}
#mailList th:first-child {
	width: 1%;
}
#mailList td {
	border-top: 1px solid #eee;
	cursor: pointer;
}
#mailList .current td {
	background: #f4f9ed;
	font-weight: bold;
}

/* mailDetail */
#mailDetail {
	padding: 10px;
}
#mailDetail header {
	position: relative;
	margin: 0 0 10px;
	padding: 0 0 10px;
	border-bottom: 1px solid #eee;
}
#mailDetail header h1 {
	margin: 0 150px 7px 0;
	font-size: 15px;
}
#mailDetail header li {
	margin: 0 0 3px;
}
#mailDetail header li span {
	margin: 0 5px 0 0;
	color: #999;
}
#mailDetail header p {
	position: absolute;
	top: 0;
	right: 0;
}
#mailDetail .body {
	line-height: 1.3;
}

/* --------------------------------------------------
	以下、サンプルコード
-------------------------------------------------- */

#contents {
	/* Old */
	display: -webkit-box;
	display: -moz-box;
	display: -ms-box;
	display: box;
	/* New */
	display: -webkit-flexbox;
	display: -moz-flexbox;
	display: -ms-flexbox;
	display: flexbox;
	
	width: 100%;
}
#mainx {
	/* Old */
	-webkit-box-ordinal-group: 2;
	-moz-box-ordinal-group: 2;
	-ms-box-ordinal-group: 2;
	box-ordinal-group: 2;
	/* New */
	-webkit-flex-order: 2;
	-moz-flex-order: 2;
	-ms-flex-order: 2;
	flex-order: 2;
	
	/* Old */
	-webkit-box-flex: 1;
	-moz-box-flex: 1;
	-ms-box-flex: 1;
	box-flex: 1;
	/* New */
	width: -webkit-flex(1);
	width: -moz-flex(1);
	width: -ms-flex(1);
	width: flex(1);
	
	background: #fff;
	word-wrap: break-word;
}
#sub {
	/* Old */
	-webkit-box-ordinal-group: 1;
	-moz-box-ordinal-group: 1;
	-ms-box-ordinal-group: 1;
	box-ordinal-group: 1;
	/* New */
	-webkit-flex-order: 1;
	-moz-flex-order: 1;
	-ms-flex-order: 1;
	flex-order: 1;
	
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	
	width: 150px;
	padding: 10px;
	background: #edebe6;
}
#subMenu {
	float: left;
}

/* operationPanel */
#subMenu {
	overflow: hidden;
	padding: 5px 5px 0;
#	background: #D6E1C7;
#	background: #45C;
}
#subMenu ul {
	float: left;
	margin: 0 10px 0 0;
}
#subMenu li {
	float: left;
	margin: 0 5px 0 0;
	padding: 0 0 5px 0;
}
#subMenu a {
	display: block;
	padding: 5px;
	background: #fff;
	border-radius: 3px;
	color: #403B33;
	text-decoration: none;
}
#extra {
	/* Old */
	-webkit-box-ordinal-group: 3;
	-moz-box-ordinal-group: 3;
	-ms-box-ordinal-group: 3;
	box-ordinal-group: 3;
	/* New */
	-webkit-flex-order: 3;
	-moz-flex-order: 3;
	-ms-flex-order: 3;
	flex-order: 3;
	
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	
	width: 200px;
	padding: 10px;
	background: #eee;
}
@media screen and (max-width: 800px) {
	#extra {
		display: none;
	}
}

/* inqList */
#inqList {
	border-bottom: 2px solid #D6E1C7;
}
#inqList table {
	width: 100%;
}
#inqList th,
#inqList td {
