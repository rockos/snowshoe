@charset "UTF-8";

/* //////////////////////////////////////////////////
[Base] 
////////////////////////////////////////////////// */

/* for IE7: page zoom bug fix */
*:first-child+html body,
*:first-child+html br { letter-spacing: 0; }

/* for IE6-7: hasLayout bug fix */
h1, h2, h3, h4, h5, h6,
p, dl, dt, dd, ul, ol, li, table, th, td,
div, blockquote, pre,
form, fieldset, legend,
article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section {
	*zoom: 1;
}

/* for IE6-7: line-height bug fix */
body {
	*line-height: 1.2;
}

/* override html5reset.css */
input, select, button {
	font-size: 100%;
}
li {
	list-style: none;
}

/* //////////////////////////////////////////////////
[Layout] 
////////////////////////////////////////////////// */

body {
	color: #403B33;
	font-size: 13px;
	font-family: Arial, Helvetica, sans-serif;
	font-family: "MS PGothic", Arial, Helvetica, sans-serif\0/;
}
a {
	color: #15C;
}
a:link,
a:visited {
	text-decoration: none;
}
a:focus,
a:hover,
a:active {
	text-decoration: underline;
}

/* --------------------------------------------------
	Header
-------------------------------------------------- */

#header {
	overflow: hidden;
	padding: 10px;
	background:#5BA;
}

/* siteID */
#siteID {
	float: left;
	margin: 0 20px 0 0;
	line-height: 1;
	font-size: 36px;
	font-family: 'Questrial', sans-serif;
}
#siteID a {
	color: #403B33;
	text-decoration: none;
}

/* search */
#search {
	float: left;
}
#search .textbox {
	width: 200px;
	padding: 5px;
	border: 1px solid #ccc;
	border-radius: 3px;
	vertical-align: middle;
	-webkit-appearance: none;
	-webkit-box-sizing: contentbox;
}
#search .textbox:focus {
  outline-offset: -2px;
}
#search .textbox::-webkit-search-decoration {
  display: none;
}
#search .submit {
	padding: 4px 10px;
	background: #94C7B6;
	border: none;
	border-radius: 3px;
	color: #fff;
	font-size: 13px;
	vertical-align: middle;
	cursor: pointer;
	-webkit-appearance: none;
}

/* userInfo */
#userInfo {
	float: right;
}
#userInfo li {
	display: inline-block;
	*display: inline;
	*zoom: 1;
	margin: 0 0 0 5px;
	vertical-align: middle;
}
#userInfo .setting {
	padding: 5px;
	background: #403B33;
	border-radius: 3px;
	color: #fff;
	text-decoration: none;
}

/* --------------------------------------------------
	Contents
-------------------------------------------------- */
#contents {}
#content_login {
    width: 930px;
    height: 520px;
    margin: -200px 0 0 -475px;
    position: absolute;
    left: 50%;
    top: 50%;
    background:
/*    url(/images/cloud.png) no-repeat 560px 220px,
*/
    url(/images/cloud.png) no-repeat 600px 140px,
    url(/images/cloud.png) no-repeat -160px -120px,
    url(/images/bg.png) repeat left top;
    -webkit-box-shadow: 10px 10px 20px rgba(0, 0, 0, 0.15);
    -moz-box-shadow: 10px 10px 20px rgba(0, 0, 0, 0.15);
    -o-box-shadow: 10px 10px 20px rgba(0, 0, 0, 0.15);
    -ms-box-shadow: 10px 10px 20px rgba(0, 0, 0, 0.15);
    box-shadow: 10px 10px 20px rgba(0, 0, 0 ,0.15);
    border: 10px solid #fff;
    -webkit-border-radius: 15px;
    -moz-border-radius: 15px;
    -o-border-radius: 15px;
    border-radius: 15px;
    *background: #2badd0 url(/images/bg.png) repeat left top;/*IE6,7に適用*/
    background: #2badd0 url(/images/bg.png) repeat left top\9;/*IE8に適用*/
    behavior: url(../css/PIE.htc);/*動作させるには絶対パスに修正する*/
}
/*タイトル*/
#content_login h1 {
    padding: 70px 70px 70px 0;
    margin: 0 20px 0 0;
    position: absolute;
    right: 50%;
    top: 50px;
    background: url(/images/shadow.png) no-repeat right center;
}

/*フォーム全体を内包するdiv要素*/
#form_login {
    margin: 0 0 0 -20px;
    position: absolute;
    left: 50%;
    top: 185px;
}

/*フォーム要素の配置*/
#form_login p {
    float: left;
    margin: 0 10px 0 0;
}

/*テキストフィールドのラベル*/
#form_login label {
    display: block;
    margin: 0 0 7px 3px;
    font-family: Helvetica, Arial, sans-serif;
    font-size: 26px;
    color: #fff;
}
#content_login h1 {
    padding: 70px 70px 70px 0;
    margin: 0 20px 0 0;
    position: absolute;
    right: 50%;
    top: 115px;
    background: url(/images/shadow.png) no-repeat right center;
}
/*テキストフィールド、パスワードフィールド*/
#form_login input[type="text"], #form_login input[type="password"] {
    position:relative;
    width: 140px;
    height: 25px;
    padding: 5px 10px;
    -webkit-box-shadow: inset 1px 2px 1px rgba(0, 0, 0, 0.2);
    -moz-box-shadow: inset 1px 2px 1px rgba(0, 0, 0, 0.2);
    -o-box-shadow: inset 1px 2px 1px rgba(0, 0, 0, 0.2);
    -ms-box-shadow: inset 1px 2px 1px rgba(0, 0, 0, 0.2);
    box-shadow: inset 1px 2px 1px rgba(0, 0, 0 ,0.2);
    border: 3px solid #caeff2;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    -o-border-radius: 20px;
    -ms-border-radius: 20px;
    border-radius: 20px;
    behavior: url(../css/PIE.htc);/*動作させるには絶対パスに修正する*/
}
#form_login input[type="text"]:hover, #form_login input[type="password"]:hover {
    border-color: #d3da21;
}
#form_login input[type="text"]:focus, #form_login input[type="password"]:focus {
    border-color: #0c93c6;
}

/*イメージボタン*/
#form_login input[type="image"] {
    padding: 8px;
    position: absolute;
    bottom: 0;
    background: -webkit-linear-gradient(#e7ee2a, #c7cd1b);
    background: -moz-linear-gradient(#e7ee2a, #c7cd1b);
    background: -o-linear-gradient(#e7ee2a, #c7cd1b);
    background: -ms-linear-gradient(#e7ee2a, #c7cd1b);
    background: linear-gradient(#e7ee2a, #c7cd1b);
    -pie-background: linear-gradient(#e7ee2a, #c7cd1b);
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -o-border-radius: 50%;
    -ms-border-radius: 50%;
    border-radius: 50%;
    -webkit-box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.15);
    -moz-box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.15);
    -o-box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.15);
    -ms-box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.15);
    box-shadow: 3px 3px 5px rgba(0, 0, 0 ,0.15);
    behavior: url(../css/PIE.htc);/*動作させるには絶対パスに修正する*/
}
#form_login input[type="image"]:hover {
    background: -webkit-linear-gradient(#f9ff60, #ca9d00);
    background: -moz-linear-gradient(#f9ff60, #ca9d00);
    background: -o-linear-gradient(#f9ff60, #ca9d00);
    background: -ms-linear-gradient(#f9ff60, #ca9d00);
    background: linear-gradient(#f9ff60, #ca9d00);
    -pie-background: linear-gradient(#f9ff60, #ca9d00);
    behavior: url(../css/PIE.htc);/*動作させるには絶対パスに修正する*/
}

/*新規利用登録の吹き出し*/
#regist_login {
    position: absolute;
    right: -50px;
    top: -50px;
}
#regist_login a:hover img {
    -webkit-transform: scale(1.1, 1.1);
    -moz-transform: scale(1.1, 1.1);
    -o-transform: scale(1.1, 1.1);
    -ms-transform: scale(1.1, 1.1);
    transform: scale(1.1, 1.1);
    *zoom: 1.1;/*IE6,7に適用*/
    zoom: 1.1\9;/*IE8に適用*/
}
/*フッタ*/
#footer_login {
    width: 100%;
    padding: 60px 0 10px;
    position: absolute;
    left: 0;
    bottom: 0;
    -webkit-border-image: url(../images/halftone.png) 50 0 0 0 / 50px 0 0 0 repeat;
    -moz-border-image: url(../images/halftone.png) 50 0 0 0 / 50px 0 0 0 repeat;
    -o-border-image: url(../images/halftone.png) 50 0 0 0 / 50px 0 0 0 repeat;
    -ms-border-image: url(../images/halftone.png) 50 0 0 0 / 50px 0 0 0 repeat;
    *background: #fff;/*IE6,7に適用*/
    background: #fff\9;/*IE8に適用*/
}

/*フッタ（クレジット）*/
#footer_login p.credit {
    float: left;
    margin: 5px 0 0 15px;
    font-size: x-small;
    color: #2da6d4;
}

/*フッタ（ナビゲーション）*/
#footer_login ul {
    float: right;
    margin-right: 15px;
}
#footer_login ul li {
    display: inline;
    margin: 0 0 0 15px;
}
#footer_login ul li a {
    font-size: small;
    font-weight: bold;
    color: #2da6d4;
}
#footer ul li a:hover {
    color: #d3da21;
}

/*IE対策JavaScript用*/
#content_login .cloudForIE {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}
#content_login .cloud1 {
    background: url(../images/cloud.png) no-repeat -160px -120px;
}
#content_login .cloud2 {
    background: url(../images/cloud.png) no-repeat 560px 220px;
}
#footer_login .borderForIE {
    width: 100%;
    height: 50px;
    position: absolute;
    left: 0;
    top: -50px;
    background: url(../images/halftone.png) repeat-x left top;
}

/* --------------------------------------------------
	Footer
-------------------------------------------------- */
#footer {
	overflow: hidden;
	padding: 10px;
	border-top: 2px solid #403B33;
}

/* rulesNav */
